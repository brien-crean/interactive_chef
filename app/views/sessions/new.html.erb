<div class="container">
  <h1>Sign In</h1>
  <div class="col-md-6">

  <%= form_tag sessions_path do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, "", class: "form-control" %>
  </div>
  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>
    <%= submit_tag "Sign In", class: "" %>
  </div>
  <% end %>
</div>
